{
  "name": "rublevsky-studio-tanstack",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate:local": "wrangler d1 migrations apply rublevsky-studio-storage --local",
    "db:migrate:prod": "wrangler d1 migrations apply rublevsky-studio-storage --remote",
    "db:seed": "tsx scripts/seedDatabase.ts",
    "db:seed:prod": "tsx scripts/seedDatabase.ts --remote",
    "db:reset": "rm -rf .wrangler/state/v3/d1 drizzle && tsx src/scripts/resetDatabase.ts && pnpm db:generate && pnpm db:migrate:local",
    "db:reset:prod": "rm -rf drizzle && tsx scripts/resetDatabase.ts --remote && pnpm db:generate && pnpm db:migrate:prod",
    "email": "email dev --dir emails"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@libsql/client": "^0.15.4",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tailwindcss/postcss": "^4.1.5",
    "@tanstack/react-query": "^5.75.4",
    "@tanstack/react-query-devtools": "^5.75.4",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-router-devtools": "^1.119.1",
    "@tanstack/react-start": "^1.119.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/start": "^1.119.2",
    "better-auth": "^1.2.7",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.43.1",
    "lucide-react": "^0.507.0",
    "motion": "^12.9.4",
    "next-themes": "^0.4.6",
    "nitro-cloudflare-dev": "^0.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "unenv": "^1.10.0",
    "vaul": "^1.1.2",
    "vinxi": "0.5.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.5",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
