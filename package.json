{
	"name": "Rublevsky Studio",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"wdev": "pnpm run build && wrangler dev",
		"build": "vite build",
		"deploy": "pnpm run build && wrangler deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"db:generate": "drizzle-kit generate",
		"db:studio": "drizzle-kit studio",
		"db:migrate": "wrangler d1 migrations apply rublevsky-studio-storage --remote",
		"db:push": "drizzle-kit push",
		"email": "email dev --dir src/components/emails",
		"kill-dev": "pkill -f 'workerd serve' && pkill -f 'esbuild.*service' && lsof -ti:8787 | xargs kill -9 2>/dev/null || true",
		"lint-format": "biome check --write"
	},
